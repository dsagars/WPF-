<Window x:Class="Test.EnterData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test"
        mc:Ignorable="d"
        Title="EnterData" Height="300" Width="300">
    <Grid Height="300" Width="300">
        <Label Content="Company Detail" Height="30" Width="100" Margin="13,13,187,257" FontWeight="Bold"/>
        <Label x:Name="CompanyName" Content="Companyname" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,66,0,0"/>
        <Label x:Name="Address" Content="Address" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,105,0,0" RenderTransformOrigin="0.091,1.958"/>
        <Label x:Name="Cars" Content="Car" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="13,144,0,0" RenderTransformOrigin="0.144,2.302"/>
        <TextBox x:Name="textbox1" HorizontalAlignment="Left" Height="23" Margin="125,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textbox2" HorizontalAlignment="Left" Height="23" Margin="125,111,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textbox3" HorizontalAlignment="Left" Height="23" Margin="125,148,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Left" Margin="169,210,0,0" VerticalAlignment="Top" Width="75" Click="Save_Click"/>
    </Grid>


</Window>
